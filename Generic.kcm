    alt:                                ';'
}

key APOSTROPHE {
    label:                              '`'
    number:                             '\''
    base:                               '\u0300'  # grave
    shift:                              '\u0323'  # underdot
    alt:                                '\''
 
}

# all dead keys, I'll leave them alone…
key STAR {
    label:                              '*'
    number:                             '*'
    base:                               '*'
    shift:                              '*'
