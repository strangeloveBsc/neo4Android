#
# Emulator keyboard character map #2.
#

type ALPHA

### Note to self: alt = M3, ctrl = M4

# base 				= layer 1
# shift, capslock 	= layer 2
# alt 				= layer 3
# ctrl 				= layer 4
# shift+ctrl		= layer 5
# alt+ctrl			= layer 6

key A {
    label:                              'A'
    number:                             '6'
    base:                               'a'   
	shift, capslock:                    'A'   # layer 2
    alt:                                '{'   # layer 3
    ctrl:								fallback DPAD_DOWN  # arrow down
    shift+ctrl:							'\u03B1'  # alpha
    ctrl+alt:							'\u2200'  # for all
}

key B {
    label:                              'B'
    number:                             none
    base:                               'b'
    shift, capslock:                    'B'
    alt:                                '+'
	ctrl:								':' 
    shift+ctrl:							'\u03B2'  # beta
    ctrl+alt:							'\u21D0'  # <=
}

key C {
    label:                              'C'
    number:                             none
    base:                               'c'
    shift, capslock:                    'C'
    alt:                                ']'
	ctrl:								'\u007F' # delete
    shift+ctrl:							'\u03C7'  # chi
    ctrl+alt:							'\u2102'  # ℂ
}

key D {
    label:                              'D'
    number:                             none
    base:                               'd'
    shift, capslock:                    'D'
    alt:                                ':'
	ctrl:								',' 
    shift+ctrl:							'\u03B4'  # delta
    ctrl+alt:							'\u0394'  # Delta
}

key E {
    label:                              'E'
    number:                             none
    base:                               'e'
    shift, capslock:                    'E'
    alt:                                '}'
	ctrl:								fallback DPAD_RIGHT
	shift+ctrl:							'\u03B5'  # epsilon
    ctrl+alt:							'\u2203'  # there exists
}

key F {
    label:                              'F'
    number:                             '9'
    base:                               'f'
    shift, capslock:                    'F'
    alt:                                '='
    ctrl:								'9'
    shift+ctrl:							'\u03C6'  # phi
    ctrl+alt:							'\u03A6'  # Phi
}

key G {
    label:                              'G'
    number:                             '8'
    base:                               'g'
    shift, capslock:                    'G'
    alt:                                '>'
    ctrl:								'8'
	shift+ctrl:							'\u03B3'  # gamma
    ctrl+alt:							'\u0393'  # Gamma
}

key H {
    label:                              'H'
    number:                             '7'
    base:                               'h'
    shift, capslock:                    'H'
    alt:                                '<'
    ctrl:								'7'
    shift+ctrl:							'\u03C8'  # psi
    ctrl+alt:							'\u03A8'  # Psi
}

key I {
    label:                              'I'
    number:                             '5'
    base:                               'i'
    shift, capslock:                    'I'
    alt:                                '/'
    ctrl:								fallback DPAD_LEFT
    shift+ctrl:							'\u03B9'  # jota
    ctrl+alt:							'\u222B'  # integral
}

key J {
    label:                              'J'
    number:                             '0'
    base:                               'j'
    shift, capslock:                    'J'
    alt:                                ';'
    ctrl: 								';'
    shift+ctrl:							'\u03B8'  # theta
    ctrl+alt:							'\u0398'  # Theta
}

key K {
    label:                              'K'
    number:                             none
    base:                               'k'
    shift, capslock:                    'K'
    alt:                                '!'
    ctrl: 								'\u00A1'
    shift+ctrl:							'\u03BA'  # kappa
    ctrl+alt:							'\u2A2F'  # cross product
}

key L {
    label:                              'L'
    number:                             '9'
    base:                               'l'
    shift, capslock:                    'L'
    alt:                                '['
    ctrl: 								fallback DPAD_UP
    shift+ctrl:							'\u03BB'  # lambda
    ctrl+alt:							'\u039B'  # Lambda
}

key M {
    label:                              'M'
    number:                             '1'
    base:                               'm'
    shift, capslock:                    'M'
    alt:                                '%'
    ctrl: 								'1'
    shift+ctrl:							'\u03BC'  # mu
    ctrl+alt:							'\u21D4'  # <=>
}

key N {
    label:                              'N'
    number:                             '4'
    base:                               'n'
    shift, capslock:                    'N'
    alt:                                '('
    ctrl: 								'4'
    shift+ctrl:							'\u03BD'  # nu
    ctrl+alt:							'\u2115'  # Natural numbers
}

key O {
    label:                              'O'
    number:                             '6'
    base:                               'o'
    shift, capslock:                    'O'
    alt:                                '*'
    ctrl: 								none
    shift+ctrl:							'\u03BF'  # omicron
    ctrl+alt:							'\u2208'  # Element of
}

key P {
    label:                              'P'
    number:                             none
    base:                               'p'
    shift, capslock:                    'P'
    alt:                                '~'
    ctrl: 								'\n' # line feed
    shift+ctrl:							'\u03C0'  # pi
    ctrl+alt:							'\u03A0'  # Pi
}

key Q {
    label:                              'Q'
    number:                             none
    base:                               'q'
    shift, capslock:                    'Q'
    alt:                                '&'
    ctrl: 								'+'
    shift+ctrl:							'\u03D5'  # phi
    ctrl+alt:							'\u211A'  # Rational numbers
}

key R {
    label:                              'R'
    number:                             '5'
    base:                               'r'
    shift, capslock:                    'R'
    alt:                                ')'
    ctrl: 								'+'
    shift+ctrl:							'\u03C1'  # rho
    ctrl+alt:							'\u211D'  # Real numbers
}

key S {
    label:                              'S'
    number:                             none
    base:                               's'
    shift, capslock:                    'S'
    alt:                                '?'
    ctrl: 								'\u00BF'  # inverted question mark
    shift+ctrl:							'\u03C3'  # sigma
    ctrl+alt:							'\u03A3'  # Sigma
}

key T {
    label:                              'T'
    number:                             '6'
    base:                               't'
    shift, capslock:                    'T'
    alt:                                '-'
    ctrl: 								'6'
    shift+ctrl:							'\u03C4'  # tau
    ctrl+alt:							'\u2202'  # part. differential
}

key U {
    label:                              'U'
    number:                             '4'
    base:                               'u'
    shift, capslock:                    'U'
    alt:                                '\\'
    ctrl: 								none
    shift+ctrl:							none
    ctrl+alt:							'\u2282'  #  subset of
}

key V {
    label:                              'V'
    number:                             '8'
    base:                               'v'
    shift, capslock:                    'V'
    alt:                                '_'
    ctrl: 								'\u0008'
    shift+ctrl:							none
    ctrl+alt:							'\u221A'  # Square root
}

key W {
    label:                              'W'
    number:                             none
    base:                               'w'
    shift, capslock:                    'W'
    alt:                                '^'
    ctrl: 								none
    shift+ctrl:							'\u03C9'  # omega
    ctrl+alt:							'\u03A9'  # Omega
}

key X {
    label:                              'X'
    number:                             '7'
    base:                               'x'
    shift, capslock:                    'X'
    alt:                                '\u2026'
    ctrl: 								none
    shift+ctrl:							'\u03BE'  # xi
    ctrl+alt:							'\u039E'  # Xi
}

key Y {
    label:                              'Y'
    number:                             none
    base:                               'y'
    shift, capslock:                    'Y'
    alt:                                '@'
    ctrl: 								'.'
    shift+ctrl:							'\u03C5'  # upsilon
    ctrl+alt:							'\u25BD'  # white triangle down
}

key Z {
    label:                              'Z'
    number:                             '9'
    base:                               'z'
    shift, capslock:                    'Z'
