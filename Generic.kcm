    ctrl: 								none
    shift+ctrl:							'\u03B6'  # zeta
    ctrl+alt:							'\u2124'  # Integer
}

key COMMA {
    label:                              ','
    number:                             '2'
    base:                               ','
    shift:                              '\u2013'
    alt:                                '"'
    ctrl: 								'2'
    shift+ctrl:							'\u03F1'  # rho
    ctrl+alt:							'\u21D2'  # =>
}

key PERIOD {
    label:                              '.'
    number:                             '3'
    base:                               '.'
    shift:                              '\u2022'
    alt:                                '\''
    ctrl: 								'3'
    shift+ctrl:							'\u03D1'  # theta
    ctrl+alt:							'\u21A6'  # |->
}

key AT {
    label:                              '@'
    number:                             '@'
    base:                               '@'
    shift:                              '@'
    alt:                                '@'
# doesn't exist?
}

key SLASH {
    label:                              '/'
    number:                             '/'
    base:                               '/'
    shift:                              '?'
    alt:                                '?'
    ctrl: 								none
# doesn't exist?
}

key SPACE {
    label:                              ' '
    number:                             ' '
    base:                               ' '
    shift:                              ' '
    alt:                                '\uef01'
    ctrl: 								'0'
    shift+ctrl:							'\u00A0'  # nbsp
    ctrl+alt:							'\u202F'  # narrow nbsp
}

key ENTER {
    label:                              '\n'
    number:                             '\n'
    base:                               '\n'
    shift:                              '\n'
    alt:                                '\n'
    ctrl: 								'\n'
    shift+ctrl:							'\n'
    ctrl+alt:							'\n'
}

key TAB {
    label:                              '\t'
    number:                             '\t'
    base:                               '\t'
    shift:                              '\u21E4'  # leftwards tab 
    alt:                                '\u266B'  # beamt eight note
    ctrl: 								'\t'
    shift+ctrl:							'\t'
    ctrl+alt:							'\t' 
}

key 0 {
    label:                              '0'
    number:                             '0'
    base:                               '0'
    shift:                              '\u201D'  # r double quot mark
    alt:                                '\u2019'  # r single quot mark
    ctrl: 								'*'
    shift+ctrl:							'\u2080'  # subscript zero
    ctrl+alt:							'\u2205'  # empty set
}

key 1 {
    label:                              '1'
    number:                             '1'
    base:                               '1'
    shift:                              '\u00B0'  # Degree
    alt:                                '\u00B9'  # Superscript 1
    ctrl: 								'\u00AA'  # Feminine ord. indicator
    shift+ctrl:							'\u2081'  # Subscript one
    ctrl+alt:							'\u00AC'  # NOT sign
}

key 2 {
    label:                              '2'
    number:                             '2'
    base:                               '2'
    shift:                              '\u00A7'  # § section sign
    alt:                                '\u00B2'  # Superscript 2
    ctrl: 								'\u00BA'  # Masculine ord. indicator
    shift+ctrl:							'\u2082'  # Subscript two
    ctrl+alt:							'\u2228'  # OR sign
}

key 3 {
    label:                              '3'
    number:                             '3'
    base:                               '3'
    shift:                              '\u2113'  # small script l
    alt:                                '\u00B3'  # superscript 3
    ctrl: 								'\u2116'  # Numero sign
    shift+ctrl:							'\u2083'  # subscript 3
    ctrl+alt:							'\u2227'  # AND sign
}

key 4 {
    label:                              '4'
    number:                             '4'
    base:                               '4'
    shift:                              '\u00BB' # >>
    alt:                                '\u203A' # >
    ctrl: 								none
    shift+ctrl:							'\u2640'  # female sign
    ctrl+alt:							'\u22A5'  # up tack
}

key 5 {
    label:                              '5'
    number:                             '5'
    base:                               '5'
    shift:                              '\u00AB'  # <<
    alt:                                '\u2039'  # <
    ctrl: 								'\u00B7'  # middle dot
    shift+ctrl:							'\u2642'  # male sign
    ctrl+alt:							'\u2221'  # Angle sign
}

key 6 {
    label:                              '6'
    number:                             '6'
    base:                               '6'
    shift:                              '$'
    alt:                                '\u002A'  # cent sign
    ctrl: 								'\u00A3'  # pound sign
    shift+ctrl:							'\u26A5'  # male + female sign
    ctrl+alt:							'\u2225'  # parallel sign
}

key 7 {
    label:                              '7'
    number:                             '7'
    base:                               '7'
    shift:                              '\u20AC'  # Euro sign
    alt:                                '\u00A5'  # Yen sign
    ctrl: 								'\u00A4'  # currency sign
    shift+ctrl:							'\u03F0'  # kappa
    ctrl+alt:							'\u2192'  # ->
}

key 8 {
    label:                              '8'
    number:                             '8'
    base:                               '8'
    shift:                              '\u201E'  # double low quot
    alt:                                '\u201A'  # single low quot
    ctrl: 								'\t'
    shift+ctrl:							'\u27E8'  # Left angle bracket
    ctrl+alt:							'\u221E'  # Infinity sign
}

key 9 {
    label:                              '9'
    number:                             '9'
    base:                               '9'
    shift:                              '\u201C'  # Left double quot
    alt:                                '\u2018'  # Left single quot
    ctrl: 								'\u002F'  # Solidus
    shift+ctrl:							'\u27E9'  # Right angle bracket
    ctrl+alt:							'\u221D'  # Proportional sign
}

key GRAVE {
    label:                              '^'
    number:                             '^'
    base:                               '\u0302'
    shift:                              '\u02C7'  # Caron
    alt:								'\u21BB'  # Clockwise open circle arrow
    ctrl: 								'\u02D9'  # Dot above
    shift+ctrl:							'\u02DE'  # Rhotic hook
    ctrl+alt:							'\u0323'  # Dot below
}

key MINUS {
    label:                              '-'
    number:                             '-'
    base:                               '-'
    shift:                              '-'

}

key EQUALS {
    label:                              '='
